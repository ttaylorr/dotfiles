[user]
	name = Taylor Blau
	email = me@ttaylorr.com
	signingkey = 8E49643D9EC651770C52C78522A953E9A217D0CE
[alias]
	st = status
	a = add
	aa = add --all
	ci = commit
	br = branch
	co = checkout
	coi = checkout-interactive
	cop = checkout-pr
	di = diff
	lb = checkout -
	up = pull --rebase --autostash
[push]
	default = current
[pull]
	default = current
[branch]
	autosetuprebase = always
[core]
	excludesfile = ~/.gitignore
[credential]
	helper = osxkeychain
[diff]
	compactionHeuristic = true
[github]
	user = ttaylorr
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[gpg]
	program = gpg
[sendemail]
  from = Taylor Blau <me@ttaylorr.com>
  smtpserver = "/usr/local/bin/msmtp"
  smtpserveroption = "-a"
  smtpserveroption = "ttaylorr.com"
